  FFWhite(60, 'p');  //วิ่งตามเส้นจนกว่าเซนเซอร์ทั้งหมดไม่เจอเส้น
  FFWhite(60, 'f');  //วิ่งตามเส้นจนกว่าเซนเซอร์ทั้งหมดไม่เจอเส้น
  FFWhite(60, 'r');  //วิ่งตามเส้นจนกว่าเซนเซอร์ทั้งหมดไม่เจอเส้น
  FFWhite(60, 'l');  //วิ่งตามเส้นจนกว่าเซนเซอร์ทั้งหมดไม่เจอเส้น


  //FFBlack(ความเร็วมอเตอร์, คำสั่งที่ใช้เจอแยก);
  FFBlack(60, 's');  // วิ่งตรงธรรมดาจนกว่าจะเส้น
  FFBlack(60, 'p');  // วิ่งตรงธรรมดาจนกว่าจะเส้น
  FFBlack(60, 'f');  // วิ่งตรงธรรมดาจนกว่าจะเส้น
  FFBlack(60, 'r');  // วิ่งตรงธรรมดาจนกว่าจะเส้น
  FFBlack(60, 'l');  // วิ่งตรงธรรมดาจนกว่าจะเส้น

  //FFTimer(ความเร็วมอเตอร์, เวลาที่ใช้วิ่ง 1000 = 1);
  FFTimer(60, 1000);  //วิ่งตามเส้นจนกว่าหมดเวลา

  //servo(ยก ลง, คีบ อ้า);
  servo(50, 40);  //เซอร์โวเริ่มต้น+
}